@model IEnumerable<Model.Custom.ProyectosGrid>

@{
    ViewBag.Title = "Proyectos Asociados";
}

<h2>Proyectos Asociados</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id_Proyecto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Titulo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaInicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaFin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Costo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Id_Cliente)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Proyecto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Titulo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @item.FechaInicio.ToString("dd/MM/yyyy")
        </td>
        <td>
            @item.FechaFin.ToString("dd/MM/yyyy")
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Costo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Cliente)
        </td>
        @*<td>
            @Html.ActionLink("Editar", "Edit", new {  id=item.Id_Proyecto  }, new { @class = "btn" }) |
            @Html.ActionLink("Detalles", "Details", new {  id=item.Id_Proyecto  }, new { @class = "btn" }) |
            @Html.ActionLink("Eliminar", "Delete", new {  id=item.Id_Proyecto  }, new { @class = "btn" })
        </td>*@
    </tr>
}

</table>

<div>
    <a class="btn" href="javascript:void(0);" onclick="history.go(-1);">Volver a la Lista</a>
</div>